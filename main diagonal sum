public class Solution {
    // DO NOT MODIFY THE ARGUMENTS WITH "final" PREFIX. IT IS READ ONLY
    public int solve(final int[][] A) {
        int sum = 0;
        
        // Loop through the diagonal and sum up the elements
        for (int i = 0; i < A.length; i++) {
            sum += A[i][i];
        }
        
        return sum;
    }
}
